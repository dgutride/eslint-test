{
  "plugins": [
    "markdown",
    "import",
    "unused-imports",
    "react"
  ],
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "eol-last": 2,
    "no-console": 2,
    "spaced-comment": 2,
    "no-unused-vars": 2,
    "react/jsx-uses-react": "error",
    "react/jsx-uses-vars": "error",
    "react/no-unknown-property": 2,
    "react/jsx-no-undef": 2
  }
}
